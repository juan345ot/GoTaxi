{
  "name": "go-taxi-backend",
  "version": "1.0.0",
  "description": "Backend profesional para GoTaxi: Express.js + MongoDB + JWT + WebSocket + Seguridad PRO",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "cross-env NODE_ENV=test jest --config ./tester/jest.config.js --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest --config ./tester/jest.integration.config.js --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "build": "echo 'Backend build completed'",
    "migrate": "echo 'No migrations to run'"
  },
  "author": "Juan Scapellato",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.8.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^1.5.13",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "redis": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.0.0",
    "supertest": "^6.3.4"
  }
}
